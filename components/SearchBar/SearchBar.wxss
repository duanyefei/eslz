/* components/SearchBar/SearchBar.wxss */
.searchBar {
  height: 88rpx;
  width: 100%;
  background-color: #f05773;
  position: absolute;
  top: 0;
  border-bottom: 1px solid #e9e9e9;
  z-index: 100;
  overflow: hidden
}
.scanButton {
  width: 88rpx;
  height: 88rpx;
  float: left;
}
.searchBar .searchInput {
  float: right;
  width: 610rpx;
  height: 64rpx;
  border-radius: 532rpx;
  background-color: rgba(255, 255, 255, .3);
  margin-top: 12rpx;
  margin-right: 30rpx;
}
.searchBar .iconSearch {
  width: 64rpx;
  height: 64rpx;
  margin-left: 8rpx;
  float: left;
}
.searchBar .keywords {
  float: left;
  color: #fff;
  font-size: 28rpx;
  line-height: 64rpx;
}